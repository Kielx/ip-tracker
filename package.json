{
  "name": "ip-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "leaflet": "^1.7.1",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": ">=3.1.0 <3.2.0 || ^3.2.1",
    "@react-leaflet/core": ">=1.0.0 <1.1.0 || ^1.1.1",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.2.7",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run watch:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/tailwind.css -o src/index.css",
    "watch": "chokidar \"./src/**/*.js\" -c \"yarn run watch:css\"",
    "dev": "concurrently \"yarn run watch\" \"yarn run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": 
[
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],

  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.2.1",
    "postcss-cli": "^8.3.1"
  }
}
